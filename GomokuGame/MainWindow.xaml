<Window x:Class="GomokuGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GomokuGame"
        mc:Ignorable="d"
        Title="MainWindow" Height="750" Width="900">
    <StackPanel>
        <Menu Margin="0 0 0 10">
            <MenuItem Header="Начать заново" Click="ResetGame_Click"/>
        </Menu>
        <WrapPanel x:Name="main" Orientation="Horizontal">
        </WrapPanel>
    </StackPanel>
</Window>
